font-family = "Bizin Gothic NF"
font-feature = -dlig
font-size = 16
background-opacity = 0.8
background-blur-radius = 20
theme = "catppuccin-mocha"

window-height = 65
window-width = 205

shell-integration-features = no-cursor
cursor-style = block
cursor-style-blink = false
cursor-text = #000000

scrollback-limit = 5000

keybind = shift+cmd+p=previous_tab
keybind = shift+cmd+n=next_tab


